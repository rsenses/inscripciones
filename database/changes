$registrations = App\Models\Registration::all();

$checkouts = App\Models\Checkout::all();

foreach($registrations as $registration) {
    $id = $registration->checkout() ? $registration->checkout()->id : null;
    echo $id . "\n";
    if ($id) {
        $registration->checkout_id = $id;
    }
    $registration->save();
}

foreach($checkouts as $checkout) {
    $checkout->products()->save($checkout->product);
}